# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'BackYard' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BackYard
	pod 'FBSDKCoreKit'
	pod 'FBSDKShareKit'
	pod 'FBSDKLoginKit'
	pod 'Firebase/Auth'
	pod 'Firebase/Core'
	pod 'Firebase/Database'
	pod 'LBTAComponents'
	pod 'SwiftyJSON'
  target 'BackYardTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BackYardUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
